{
  "name": "pollen-solutions/coming-soon",
  "description": "Coming Soon Component",
  "homepage": "https://github.com/milkcreation/pollen-coming-soon.git",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "coming soon",
    "landing page"
  ],
  "time": "2021-09-14",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "require": {
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\ComingSoon\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\ComingSoon\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config" : {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
